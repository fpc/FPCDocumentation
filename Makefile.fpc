ifndef CSSFILE
        cp fpdoc.cst fclres/fpdoc.css
endif
        @$(ECHO) '' > fclres.chk

rtl.chk: $(RTLXML) rtl-project.xml
        $(FPDOC) $(FPDOCOPTS) --project=rtl-project.xml --format=$(HTMLFMT) --output=rtl$(HTMLSUFFIX) $(FPDOCHTMLOPTS) $(RTLCHMOPTS)
ifndef CSSFILE
        cp fpdoc.cst rtl/fpdoc.css
endif
        @$(ECHO) '' > rtl.chk

